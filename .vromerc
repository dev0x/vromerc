map j 10j
map k 10k

"Manipulate tabs
map <C-j> gt
map <C-k> gT
map h gT
map l gt

"delete a tab 
nmap d D

"remove stupid tab mapping
unmap O
"map it appropriately to O
nmap O <C-t>

set showstatus=1

"disable sites for better shortcuts
set disablesites=mail.google.com

set enable_vrome_key=<Esc>

"set up custom search engines
set searchengines={"duck":"http://duckduckgo.com/?q={{keyword}}"}
"default to duckduckgo
set defaultsearch=duck

set useletters=1
set hintkeys=jklhfdgsweui


begin_custom_css
.__vim_hint_highlight_span {
text-transform: uppercase;
color:  green
}
