map j 10j
map k 10k

"Manipulate tabs
map <C-j> gt
map <C-k> gT
map h gT
map l gt

"delete a tab 
map d D

"remove stupid tab mapping
unmap T
unmap O
"map it appropriately to O
map <C-t> O

set showstatus=1

"disable sites for better shortcuts
set disablesites=mail.google.com, jht.ontimenow.com



"set up custom search engines
set searchengines={"duck":"http://duckduckgo.com/?q={{keyword}}","google":"http://www.google.com/search?/q={{keyword}}","answers":"http://www.answers.com/main/ntquery?s={{keyword}}"}
"default to duckduckgo
set defaultsearch=duck

set useletters=1
set hintkeys=jklhfdgsweui


begin_custom_css
.__vim_hint_highlight_span {
text-transform: uppercase;
color:  green
}
